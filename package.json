    "@sendgrid/mail": "^6.2.1",
    "axios": "^0.18.0",
    "bcrypt": "^2.0.0",
    "body-parser": "^1.18.2",
    "change-case": "^3.0.2",
    "cloudinary": "^1.11.0",
    "express": "^4.16.3",
    "express-json-validator-middleware": "^1.2.1",
    "express-oauth-server": "^2.0.0",
    "http-status-codes": "^1.3.0",
    "libphonenumber-js": "^1.1.11",
    "lodash": "^4.17.5",
    "mongoose": "^5.0.15",
