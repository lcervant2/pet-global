{
  "name": "pet-globe",
  "version": "0.1.0",
  "description": "API server for the PetGlobe service",
  "main": "bin/www",
  "scripts": {
    "server": "node bin/www",
    "client": "node scripts/start-client.js",
    "start": "concurrently --kill-others-on-fail \"nodemon bin/www\" \"npm run client\"",
    "build": "node scripts/build.js",
